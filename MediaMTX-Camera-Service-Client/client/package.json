{
  "name": "mediamtx-camera-client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.8.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test:unit": "npx jest --config tests/config/jest-unit.config.cjs",
    "test:integration": "jest --config tests/config/jest-integration.config.cjs",
    "test:e2e": "jest --config tests/config/jest-e2e.config.cjs",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test": "npm run test:unit",
    "test:duplication-check": "node scripts/detect-duplications.js",
    "test:duplication-check-quick": "node scripts/pre-commit-duplication-check.js",
    "ci:duplication": "npm run test:duplication-check",
    "ci:all": "npm run lint && npm run type-check && npm run test:all && npm run ci:duplication"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-router-dom": "^6.20.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-react": "^4.1.1",
    "eslint": "^8.54.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "prettier": "^3.1.0",
    "typescript": "^5.3.3",
    "vite": "^5.4.19",
    "ws": "^8.18.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "babel-jest": "^29.7.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "identity-obj-proxy": "^3.0.0",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "husky": "^8.0.3"
  }
}
